{
  "name": "urtext-piano",
  "productName": "Urtext Piano",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": "src/index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"wait-on http://localhost:3000 && electron .\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:3000 && electron .",
    "electron:build": "npm run build && electron .",
    "build:preload": "npx esbuild src/main/preload.ts --bundle --platform=node --format=cjs --target=node16 --outfile=dist/preload.js --external:electron",
    "preelectron": "npm run build:preload",
    "start": "npm run build && electron-forge start",
    "package": "npm run build && electron-forge package",
    "make": "npm run build && electron-forge make",
    "publish": "npm run build && electron-forge publish",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [],
  "author": {
    "name": "frusaha",
    "email": "118129896+frusaha@users.noreply.github.com"
  },
  "license": "MIT",
  "dependencies": {
    "dexie": "^4.0.11",
    "electron-squirrel-startup": "^1.0.1",
    "fast-xml-parser": "^4.5.3",
    "opensheetmusicdisplay": "^1.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "unzipper": "^0.10.14",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/electron": "^1.4.38",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/unzipper": "^0.10.11",
    "@vitejs/plugin-react": "^4.5.2",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^36.5.0",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.25.5",
    "jest": "^30.0.2",
    "jest-axe": "^10.0.0",
    "jest-environment-jsdom": "^30.0.2",
    "npm-run-all": "^4.1.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "wait-on": "^8.0.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.44.1"
  },
  "overrides": {
    "rollup": "npm:@rollup/wasm-node"
  },
  "build": {
    "appId": "com.frusaha.urtext-piano",
    "productName": "Urtext Piano",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/**/*",
      "src/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  }
}
